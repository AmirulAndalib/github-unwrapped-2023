    "@remotion/shapes": "4.0.74",
    "@remotion/transitions": "4.0.74",
    "@sentry/cli": "^2.22.3",
    "@sentry/node": "^7.83.0",
    "@sentry/profiling-node": "^1.2.6",
    "@tanstack/react-router": "^0.0.1-beta.212",
    "@types/body-parser": "^1.19.5",
    "@types/express": "^4.17.21",
